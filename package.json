{
  "name": "vapi-voice-agent",
  "version": "1.0.0",
  "description": "VAPI Voice Agent with Mock CRM Integration",
  "main": "dist/index.js",
  "scripts": {
    "dev:lead-crm": "ts-node-dev --respawn --transpile-only src/mock-servers/leadCrm.server.ts",
    "dev:userdata-crm": "ts-node-dev --respawn --transpile-only src/mock-servers/userDataCrm.server.ts",
    "dev:classification-crm": "ts-node-dev --respawn --transpile-only src/mock-servers/classificationCrm.server.ts",
    "dev:vici": "ts-node-dev --respawn --transpile-only src/mock-servers/vici.server.ts",
    "dev:vapi-handler": "ts-node-dev --respawn --transpile-only src/vapi/toolHandler.server.ts",
    "dev:all": "concurrently \"npm run dev:lead-crm\" \"npm run dev:userdata-crm\" \"npm run dev:classification-crm\" \"npm run dev:vici\" \"npm run dev:vapi-handler\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vapi",
    "voice-agent",
    "crm",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vapi-ai/server-sdk": "^0.10.2",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "concurrently": "^8.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
